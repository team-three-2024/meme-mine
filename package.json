  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@react-three/drei": "8.11.1",
    "@react-three/fiber": "7.0.26",
    "@react-three/postprocessing": "2.1.6",
    "@types/three": "0.136.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.19.0",
    "face-api.js": "^0.22.2",
    "react": "*",
    "react-dom": "*",
    "react-hot-toast": "^2.4.1",
    "styled-components": "^5.3.0",
    "three": "0.136.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "^7.15.6",
